{
  "name": "@replit/protocol",
  "version": "0.2.89",
  "description": "JavaScript bindings for the Replit protocol",
  "main": "main/index.js",
  "repository": "https://github.com/replit/protocol",
  "author": "faris@repl.it",
  "license": "MIT",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "npm run build:main && npm run build:iotester",
    "build:iotester": "TARGET='./iotester/index' ./gen.sh nofix ../../cmd/io-tester/lib/api/api.proto",
    "build:main": "TARGET='./main/index' ./gen.sh fix ../types.proto ../client.proto ../signing.proto ../lore.proto ../features/features.proto ../repl/repl.proto"
  },
  "dependencies": {
    "protobufjs": "^6.11.3"
  },
  "devDependencies": {
    "flowgen": "~1.14.0"
  }
}
